# Base URL for API (used by buy() etc.; no trailing slash needed)
BASE_URL=https://rugplay.com/

# Enable buying
BUY_ENABLED=true

# Buy (after claim): coin symbol and amount
BUY_SYMBOL=

# From browser: cf_clearance cookie (Cloudflare)
CF_CLEARANCE=

# From browser: __Secure-better-auth.session_token cookie
SESSION_TOKEN=

# User-Agent (copy from browser DevTools or leave unset for default)
USER_AGENT=
